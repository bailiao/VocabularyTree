#! /bin/bash

DEFAULT_CLUSTER_METHOD=2
DEFAULT_SCORE_METHOD=3
DEFAULT_LINKAGE_METHOD="complete"
MAX_CLASS_COUNT=15
PREDICT_CLASS_COUNT=7
TOL_BIAS=3

make clean
python3 ./src/test_cluster.py $DEFAULT_CLUSTER_METHOD $DEFAULT_SCORE_METHOD $DEFAULT_LINKAGE_METHOD $MAX_CLASS_COUNT $PREDICT_CLASS_COUNT $TOL_BIAS
bash ./src/classify 